{
  "version": 2,
  "builds": [
    { "src": "server.js","use": "@vercel/node" },
    { "src": "lib/**","use": "@vercel/static" },
    { "src": "node_modules/**","use": "@vercel/static" },
    { "src": "*.json","use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/","dest": "/public/index.html" },
    { "src": "/.*","dest": "/server.js" }
  ]
}
